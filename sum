import java.lang.*;
import java.util.*;

public class Main {
    public static void main(String[] args) {
        
       Scanner sc = new Scanner(System.in);
       int T = sc.nextInt();
        while(T>0){
		   int N = sc.nextInt();
        int sum=0;
        int j;
        while(N>0){
            j=N%10;
            sum=sum+j;
            
            N=N/10;

        }
		System.out.println(sum);
        T--;
        }
    
    }
}
